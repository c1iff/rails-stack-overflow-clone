<!DOCTYPE html>
<html>
  <head>
    <title>StackOverflow</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash[:notice]%>
      <%= flash[:notice] %>
    <% end %>
    <% if flash[:alert]%>
      <%= flash[:alert] %>
    <% end %>
    <% if current_user%>
      <%= current_user.email %>
    <% end %>
    <%= yield %>
  </body>
</html>
